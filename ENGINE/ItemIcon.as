package  {		import flash.display.MovieClip;			public class ItemIcon extends MovieClip {				public function ItemIcon() {			this.mouseChildren = false;			this.mouseEnabled = false;			this.txtStack.text = "";			this.txtStack.mouseEnabled = false;		}		public function set stackSize(i:int){			if(i > 1){				this.txtStack.text = String(i);			} else {				this.txtStack.text = "";			}		}		public function clearIcon(){			while(this.base.numChildren){				this.base.removeChildAt(0);			}		}		public function set icon(iconName:String){			var ic:MovieClip = Utils.getIconAsset(iconName);			if(ic != null){				this.clearIcon();				this.base.addChild(ic);			}		}		public function set rarity(level:int){			switch(level){				case 1:				//this.rarityMC.gotoAndStop("Unique");				break;				case 2:				//this.rarityMC.gotoAndStop("Rare");				break;				case 3:				this.rarityMC.gotoAndStop("Epic");				break;				case 4:				//this.rarityMC.gotoAndStop("Legendary");				break;				default:				this.rarityMC.gotoAndStop(1);				break;			}		}		public function setExtra(member:int, token:int){			if(member == 1 && token == 1){				//this.extra.gotoAndStop("Both");				return;			}			if(member == 1){				//this.extra.gotoAndStop("Member");			}			if(token == 1){				//this.extra.gotoAndStop("Token");			}		}	}	}